<!doctype html>
<html>
<head>
    <meta charset="utf-8">
    <title>Genshin Impact Randomizer</title>
    <meta property="og:title" content="Genshin Impact Randomizer" />
    <meta name="viewport" content="width=device-width">
    <link rel="icon" href="images/favicon.png">
    <script src="https://apis.google.com/js/api.js"></script>
    <script>
        let lang = localStorage.lang;

        if (lang != 'eng' && lang != 'rus') {
            let user_lang = navigator.language || navigator.userLanguage;

            if (user_lang == 'ru-RU') {
                lang = 'rus';
            } else {
                lang = 'eng';
            }
        }

        document.write('<'+ 'script' +' src="/js/lang/casino_'+ lang +'.js?<%= version %>"></' + 'script>');
    </script>
</head>
<body>
    <div id="app"></div>
</body>
</html>
